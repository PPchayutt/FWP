<!-- home.ejs -->
<!doctype html>
<html lang="en">

<%- include("head") %>

    <body>
        <div class="container" <!-- header -->
            <%- include("header") %>
                <!-- Content -->
                <table>
                    <% data.forEach((item)=>{ %>
                        <tr>

                            <td>
                                <%=item.EmployeeId%>
                            <td>
                                <%=item.FirstName%>
                            </td>
                            <td>
                                <%=item.LastName%>
                            </td>
                            <td>
                                <%=item.Title%>
                            </td>
                            <td>
                                <%=item.Phone%>
                            </td>
                            <td>
                                <%=item.Email%>
                            </td>
                            <td>
                                <a href="/delete/<%=item.EmployeeId%>">Delete</a>
                            </td>
                        </tr>
                        <br>
                        <% }); %>

                            <!-- footer -->
                            <%- include("footer") %>
                </table>
        </div>
    </body>

</html>